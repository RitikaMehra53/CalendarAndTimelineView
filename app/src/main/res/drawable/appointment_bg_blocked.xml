<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#A6A6A6"/>
            <corners
                android:topLeftRadius="@dimen/_5dp"
                android:bottomLeftRadius="@dimen/_5dp"
                android:bottomRightRadius="@dimen/_8dp"
                android:topRightRadius="@dimen/_8dp"/>
        </shape>
    </item>

    <item android:left="@dimen/_5dp">
        <shape android:shape="rectangle">
            <solid android:color="#EEEEEE"/>
            <corners android:bottomRightRadius="@dimen/_8dp"
                android:topRightRadius="@dimen/_8dp" />
        </shape>
    </item>
</layer-list>
